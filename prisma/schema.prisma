// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model DuneQuery {
  id           BigInt   @unique
  query_id     Int
  execution_id String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt

  @@index([execution_id])
  @@index([createdAt])
}
